{
  "name": "tft-scraper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint . --ext .ts",
    "build": "webpack",
    "validate": "aws cloudformation validate-template --template-body file://template.yaml",
    "make-bucket": "aws s3 mb s3://tft-legends-match-scraper-package",
    "package": "aws cloudformation package --template-file template.yaml --s3-bucket tft-legends-match-scraper-package --output-template-file packaged-template.yaml",
    "deploy": "aws cloudformation deploy --template-file packaged-template.yaml --stack-name TftLegendsMatchScraperStack --capabilities CAPABILITY_IAM CAPABILITY_AUTO_EXPAND",
    "delete": "aws cloudformation delete-stack --stack-name TftLegendsMatchScraperStack",
    "deploy:e2e": "npm run validate && npm run build && npm run package && npm run deploy",
    "test:ci": "npm run lint &&npm run test -- --coverage --coverageReporters=text-lcov",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "misc": "npx ts-node -r tsconfig-paths/register src/misc.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.17",
    "aws-sdk": "^2.1497.0",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "pg-native": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
